(deflisten window 
  :initial "..." 
  "sh $HOME/.config/eww/bars/default/widgets/hyprland/scripts/get_window_title.sh"
)
(defwidget window_w []
  (eventbox
    :class "window_w-event"
    (box
      :class "window_w"
      (label 
        :text "${window}"
        :limit-width 60
      )
    )
  )
)
